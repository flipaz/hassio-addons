{
  "name": "motionEye",
  "version": "0.10.2",
  "slug": "motioneye",
  "description": "Simple, elegant and feature-rich CCTV/NVR for your cameras",
  "url": "https://github.com/hassio-addons/addon-motioneye",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:8765]",
  "startup": "application",
  "ingress": true,
  "ingress_port": 0,
  "panel_icon": "mdi:cctv",
  "homeassistant": "0.92.0b2",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "init": false,
  "ports": {
    "80/tcp": null,
	"7999/tcp": null,
	"8081/tcp": null,
	"8082/tcp": null,
	"8083/tcp": null,
	"8084/tcp": null,
	"8085/tcp": null,
	"8086/tcp": null,
	"8087/tcp": null,
	"8088/tcp": null,
	"8089/tcp": null,
	"8090/tcp": null
  },
  "ports_description": {
    "80/tcp": "Web interface (Not required for Ingress)",
	"7999/tcp": "Web Motion interface (also enable in config)",
	"8081/tcp": "Camera streaming port",
	"8082/tcp": "Camera streaming port",
	"8083/tcp": "Camera streaming port",
	"8084/tcp": "Camera streaming port",
	"8085/tcp": "Camera streaming port",
	"8086/tcp": "Camera streaming port",
	"8087/tcp": "Camera streaming port",
	"8088/tcp": "Camera streaming port",
	"8089/tcp": "Camera streaming port",
	"8090/tcp": "Camera streaming port"
  },
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "host_network": false,
  "apparmor": false,
  "video": true,
  "privileged": [
    "DAC_READ_SEARCH",
    "SYS_ADMIN"
  ],
  "map": [
    "media:rw",
    "share:rw",
    "ssl"
  ],
  "options": {
    "motion_webcontrol": false,
    "ssl": true,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "action_buttons": []
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)?",
    "motion_webcontrol": "bool",
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str",
    "action_buttons": [
      {
        "type": "list(lock|unlock|light_on|light_off|alarm_on|alarm_off|up|right|down|left|zoom_in|zoom_out|preset1|preset2|preset3|preset4|preset5|preset6|preset7|preset8|preset9)?",
        "camera": "int?",
        "command": "str?"
      }
    ]
  },
  "image": "hassioaddons/motioneye"
}